<!-- [Gameplay] Malls - Better Build (Lion053) -->

<ModOps>

<!-- # Adjustments for Small Malls (Jakob) -->

  <!-- ProductionBuffDistance is no longer needed (ForwardBuffs are required instead) -->
   <!-- AllowNoMatch=1 since Jakob uses ProductionBuffDistance only in the first recipe-assets and uses BaseAsset for all others. All guids listed to be sure all assets are changed even if BaseAsset changes -->
  <ModOp Type="remove" GUID="1500010851,1500010853,1500010852,1500010854,1500010855,1500010857,1500010859,1500010858,1500010860,1500010861,1500010863,1500010864,1500010865,1500010866,1500010867" Path="/Values/BuffFactory/ProductionBuffDistance" AllowNoMatch="1">
  </ModOp>
  <!-- Distribute the service from the 'PublicServiceData' by street -->
   <!-- AllowNoMatch=1 since Jakob uses ProductionBuffScope in Blanks, but sometimes also in Recipe-buildings. To be sure all assets are changed, I list all guids no matter if they only inherit from BaseAsset -->
  <ModOp Type="merge" GUID="1500010850,1500010856,1500010862,1500010851,1500010853,1500010852,1500010854,1500010855,1500010857,1500010859,1500010858,1500010860,1500010861,1500010863,1500010864,1500010865,1500010866,1500010867" Path="/Values/BuffFactory" AllowNoMatch="1">
    <ProductionBuffScope>Local</ProductionBuffScope>
  </ModOp>
  <!-- Set the satisfaction distance to 19 (match Jakob's original distance) -->
  <ModOp Type="merge" GUID="1500010851,1500010853,1500010852,1500010854,1500010855,1500010857,1500010859,1500010858,1500010860,1500010861,1500010863,1500010864,1500010865,1500010866,1500010867" Path="/Values/BuffFactory/PublicServiceData/FullSatisfactionDistance">
    <FullSatisfactionDistance>19</FullSatisfactionDistance>
  </ModOp>
  <ModOp Type="merge" GUID="1500010851,1500010853,1500010852,1500010854,1500010855,1500010857,1500010859,1500010858,1500010860,1500010861,1500010863,1500010864,1500010865,1500010866,1500010867" Path="/Values/BuffFactory/PublicServiceData/NoSatisfactionDistance">
    <NoSatisfactionDistance>19</NoSatisfactionDistance>
  </ModOp>

